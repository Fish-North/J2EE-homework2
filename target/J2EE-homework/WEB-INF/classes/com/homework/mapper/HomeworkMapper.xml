<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.homework.mapper.HomeworkMapper">
    <select id="allHomework" resultType="Homework">
        select * from homework
    </select>
    <select id="queryHomeworkByID" parameterType="Integer" resultType="Homework">
        select * from homework where homework_id=#{homeworkID}
    </select>
</mapper>
